<!DOCTYPE html>
<html  data-ng-app="jDash">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <!-- STYLES -->
        <!-- build:css lib/css/main.min.css -->
        <link rel="stylesheet" type="text/css" href="{{ asset('/bundles/framework/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('/bundles/framework/font-awesome/css/font-awesome.min.css') }}">
        <!-- endbuild -->
        <!-- SCRIPTS -->
        <!-- build:js lib/js/main.min.js -->
        <script type="text/javascript" src="{{ asset('/bundles/framework/angular/angular.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/bundles/framework/angular-bootstrap/ui-bootstrap-tpls.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/bundles/framework/angular-cookies/angular-cookies.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/bundles/framework/angular-ui-router/release/angular-ui-router.min.js') }}"></script>
        <!-- endbuild -->
        <!-- Custom Scripts -->
        <script type="text/javascript" src="{{ asset('/bundles/framework/js/dashboard.min.js') }}"></script>
    </head>
    <body>
        <div class="container">
            {% block body %}{% endblock %}
            {% block javascripts %}{% endblock %}
        </div>

    </body>
</html>
